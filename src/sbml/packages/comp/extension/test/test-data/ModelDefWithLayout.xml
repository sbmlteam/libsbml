<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version1/core" xmlns:layout="http://www.sbml.org/sbml/level3/version1/layout/version1" xmlns:comp="http://www.sbml.org/sbml/level3/version1/comp/version1" level="3" version="1" layout:required="false" comp:required="true">
  <model />  
  <comp:listOfModelDefinitions>
  <comp:modelDefinition id="QuorumTrigger">
    <listOfCompartments>
      <compartment id="default" spatialDimensions="3" size="1" constant="true">
        <annotation>EnclosingCompartment</annotation>
      </compartment>
    </listOfCompartments>
    <listOfSpecies>
      <species id="Env" compartment="default" initialAmount="0" hasOnlySubstanceUnits="true" boundaryCondition="false" constant="false">
        <annotation>Type=internal</annotation>
      </species>
      <species id="LuxR" compartment="default" initialAmount="0" hasOnlySubstanceUnits="true" boundaryCondition="false" constant="false">
        <annotation>Type=internal</annotation>
      </species>
      <species id="Complex" compartment="default" initialAmount="0" hasOnlySubstanceUnits="true" boundaryCondition="false" constant="false">
        <annotation>Type=internal</annotation>
      </species>
      <species id="LuxI" compartment="default" initialAmount="0" hasOnlySubstanceUnits="true" boundaryCondition="false" constant="false">
        <annotation>Type=internal</annotation>
      </species>
      <species id="GFP" compartment="default" initialAmount="0" hasOnlySubstanceUnits="true" boundaryCondition="false" constant="false">
        <annotation>Type=internal</annotation>
      </species>
      <species id="P1" compartment="default" initialAmount="2" hasOnlySubstanceUnits="true" boundaryCondition="false" constant="false">
        <annotation>Type=Promoter</annotation>
      </species>
      <species id="P5" compartment="default" initialAmount="2" hasOnlySubstanceUnits="true" boundaryCondition="false" constant="false">
        <annotation>Type=Promoter</annotation>
      </species>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="kr_f" name="Forward repression binding rate" value="0.5" constant="true"/>
      <parameter id="kr_r" name="Reverse repression binding rate" value="1" constant="true"/>
      <parameter id="ka_f" name="Forward activation binding rate" value="0.0033" constant="true"/>
      <parameter id="ka_r" name="Reverse activation binding rate" value="1" constant="true"/>
      <parameter id="kc_f" name="Forward complex formation rate" value="0.05" constant="true"/>
      <parameter id="kc_r" name="Reverse complex formation rate" value="1" constant="true"/>
      <parameter id="ko_f" name="Forward RNAP binding rate" value="0.033" constant="true"/>
      <parameter id="ko_r" name="Reverse RNAP binding rate" value="1" constant="true"/>
      <parameter id="kao_f" name="Forward activated RNAP binding rate" value="1" constant="true"/>
      <parameter id="kao_r" name="Reverse activated RNAP binding rate" value="1" constant="true"/>
      <parameter id="kmdiff_f" name="Forward membrane diffusion rate" value="1" constant="true"/>
      <parameter id="kmdiff_r" name="Reverse membrane diffusion rate" value="0.01" constant="true"/>
      <parameter id="kd" name="Degradation rate" value="0.0075" constant="true"/>
      <parameter id="kecd" name="Extracellular degradation rate" value="0.005" constant="true"/>
      <parameter id="nc" name="Stoichiometry of binding" value="2" constant="true"/>
      <parameter id="nr" name="Initial RNAP count" value="30" constant="true"/>
      <parameter id="ko" name="Open complex production rate" value="0.05" constant="true"/>
      <parameter id="kb" name="Basal production rate" value="0.0001" constant="true"/>
      <parameter id="ng" name="Initial promoter count" value="2" constant="true"/>
      <parameter id="np" name="Stoichiometry of production" value="10" constant="true"/>
      <parameter id="ka" name="Activated production rate" value="0.25" constant="true"/>
      <parameter id="kecdiff" name="Extracellular diffusion rate" value="1" constant="true"/>
    </listOfParameters>
    <listOfReactions>
      <reaction id="Production_P1" reversible="false" fast="false" compartment="default">
        <annotation>Production</annotation>
        <listOfProducts>
          <speciesReference species="LuxR" stoichiometry="10" constant="true"/>
        </listOfProducts>
        <listOfModifiers>
          <modifierSpeciesReference species="P1">
            <annotation>promoter</annotation>
          </modifierSpeciesReference>
          <modifierSpeciesReference species="Env">
            <annotation>activation</annotation>
          </modifierSpeciesReference>
        </listOfModifiers>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> P1 </ci>
                <apply>
                  <plus/>
                  <apply>
                    <times/>
                    <ci> kb </ci>
                    <apply>
                      <divide/>
                      <ci> ko_f </ci>
                      <ci> ko_r </ci>
                    </apply>
                    <ci> nr </ci>
                  </apply>
                  <apply>
                    <times/>
                    <ci> ka </ci>
                    <apply>
                      <divide/>
                      <ci> kao_f </ci>
                      <ci> kao_r </ci>
                    </apply>
                    <ci> nr </ci>
                    <apply>
                      <power/>
                      <apply>
                        <times/>
                        <apply>
                          <divide/>
                          <ci> ka_f </ci>
                          <ci> ka_r </ci>
                        </apply>
                        <ci> Env </ci>
                      </apply>
                      <ci> nc </ci>
                    </apply>
                  </apply>
                </apply>
              </apply>
              <apply>
                <plus/>
                <cn type="integer"> 1 </cn>
                <apply>
                  <times/>
                  <apply>
                    <divide/>
                    <ci> ko_f </ci>
                    <ci> ko_r </ci>
                  </apply>
                  <ci> nr </ci>
                </apply>
                <apply>
                  <times/>
                  <apply>
                    <divide/>
                    <ci> kao_f </ci>
                    <ci> kao_r </ci>
                  </apply>
                  <ci> nr </ci>
                  <apply>
                    <power/>
                    <apply>
                      <times/>
                      <apply>
                        <divide/>
                        <ci> ka_f </ci>
                        <ci> ka_r </ci>
                      </apply>
                      <ci> Env </ci>
                    </apply>
                    <ci> nc </ci>
                  </apply>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="Complex_Complex" reversible="true" fast="false" compartment="default">
        <annotation>Complex</annotation>
        <listOfReactants>
          <speciesReference species="LuxR" stoichiometry="2" constant="true"/>
          <speciesReference species="LuxI" stoichiometry="2" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Complex" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <minus/>
              <apply>
                <times/>
                <ci> kc_f </ci>
                <apply>
                  <power/>
                  <ci> LuxR </ci>
                  <ci> nc </ci>
                </apply>
                <apply>
                  <power/>
                  <ci> LuxI </ci>
                  <ci> nc </ci>
                </apply>
              </apply>
              <apply>
                <times/>
                <ci> kc_r </ci>
                <ci> Complex </ci>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="Production_P5" reversible="false" fast="false" compartment="default">
        <annotation>Production</annotation>
        <listOfProducts>
          <speciesReference species="GFP" stoichiometry="10" constant="true"/>
          <speciesReference species="LuxI" stoichiometry="10" constant="true"/>
          <speciesReference species="LuxR" stoichiometry="10" constant="true"/>
        </listOfProducts>
        <listOfModifiers>
          <modifierSpeciesReference species="P5">
            <annotation>promoter</annotation>
          </modifierSpeciesReference>
          <modifierSpeciesReference species="Complex">
            <annotation>activation</annotation>
          </modifierSpeciesReference>
        </listOfModifiers>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> P5 </ci>
                <apply>
                  <plus/>
                  <apply>
                    <times/>
                    <ci> kb </ci>
                    <apply>
                      <divide/>
                      <ci> ko_f </ci>
                      <ci> ko_r </ci>
                    </apply>
                    <ci> nr </ci>
                  </apply>
                  <apply>
                    <times/>
                    <ci> ka </ci>
                    <apply>
                      <divide/>
                      <ci> kao_f </ci>
                      <ci> kao_r </ci>
                    </apply>
                    <ci> nr </ci>
                    <apply>
                      <power/>
                      <apply>
                        <times/>
                        <apply>
                          <divide/>
                          <ci> ka_f </ci>
                          <ci> ka_r </ci>
                        </apply>
                        <ci> Complex </ci>
                      </apply>
                      <ci> nc </ci>
                    </apply>
                  </apply>
                </apply>
              </apply>
              <apply>
                <plus/>
                <cn type="integer"> 1 </cn>
                <apply>
                  <times/>
                  <apply>
                    <divide/>
                    <ci> ko_f </ci>
                    <ci> ko_r </ci>
                  </apply>
                  <ci> nr </ci>
                </apply>
                <apply>
                  <times/>
                  <apply>
                    <divide/>
                    <ci> kao_f </ci>
                    <ci> kao_r </ci>
                  </apply>
                  <ci> nr </ci>
                  <apply>
                    <power/>
                    <apply>
                      <times/>
                      <apply>
                        <divide/>
                        <ci> ka_f </ci>
                        <ci> ka_r </ci>
                      </apply>
                      <ci> Complex </ci>
                    </apply>
                    <ci> nc </ci>
                  </apply>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="Degradation_GFP" reversible="false" fast="false" compartment="default">
        <annotation>Degradation</annotation>
        <listOfReactants>
          <speciesReference species="GFP" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> kd </ci>
              <ci> GFP </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="Degradation_LuxI" reversible="false" fast="false" compartment="default">
        <annotation>Degradation</annotation>
        <listOfReactants>
          <speciesReference species="LuxI" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> kd </ci>
              <ci> LuxI </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="Degradation_Complex" reversible="false" fast="false" compartment="default">
        <annotation>Degradation</annotation>
        <listOfReactants>
          <speciesReference species="Complex" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> kd </ci>
              <ci> Complex </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="Degradation_LuxR" reversible="false" fast="false" compartment="default">
        <annotation>Degradation</annotation>
        <listOfReactants>
          <speciesReference species="LuxR" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> kd </ci>
              <ci> LuxR </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="MembraneDiffusion_LuxI" reversible="true" fast="false" compartment="default">
        <annotation>Diffusion</annotation>
        <listOfReactants>
          <speciesReference species="LuxI" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <minus/>
              <apply>
                <times/>
                <ci> kmdiff_f </ci>
                <ci> LuxI </ci>
              </apply>
              <ci> kmdiff_r </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
    <listOfEvents>
